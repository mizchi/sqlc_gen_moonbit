pub let list_posts_with_authors_paginated_sql : String = "SELECT\n  p.id, p.title, p.view_count, p.is_published, p.created_at,\n  u.id as author_id, u.name as author_name\nFROM posts p\nINNER JOIN users u ON p.author_id = u.id\nWHERE p.id > $1\nORDER BY p.id\nLIMIT $2"

